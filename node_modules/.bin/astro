#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mereltheisen/website/mereltheisen/node_modules/.pnpm/astro@2.10.15_@types+node@17.0.45_sharp@0.31.3/node_modules/astro/node_modules:/Users/mereltheisen/website/mereltheisen/node_modules/.pnpm/astro@2.10.15_@types+node@17.0.45_sharp@0.31.3/node_modules:/Users/mereltheisen/website/mereltheisen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mereltheisen/website/mereltheisen/node_modules/.pnpm/astro@2.10.15_@types+node@17.0.45_sharp@0.31.3/node_modules/astro/node_modules:/Users/mereltheisen/website/mereltheisen/node_modules/.pnpm/astro@2.10.15_@types+node@17.0.45_sharp@0.31.3/node_modules:/Users/mereltheisen/website/mereltheisen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
